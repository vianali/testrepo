var a=[17, 24, 4, 26, 19, 30, 2, 66, 40];
var isodd=[];
var min=null;

for (i=0; i<a.length; i++){
  if(a[i]%2!==0){
    isodd.push(a[i]);
    if (min===null){
    min=a[i];
    }
    else if (a[i]<min){
    min=a[i];
    }
    
  }
  
}
console.log(isodd);
console.log("This is the minimum value="+min);